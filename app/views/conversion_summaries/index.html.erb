<p id="notice"><%= notice %></p>

<h3>Listing Conversion Summaries</h3>

<%= will_paginate @conversion_summaries, renderer: BootstrapPagination::Rails %>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Created</th>
      <th>Raw channel count</th>
      <th>Final raw channel count</th>
      <th>Starting channel count</th>
      <th>Channels created</th>
      <th>Channels skipped</th>
      <th>Final channel count</th>
      <th>Raw program count</th>
      <th>Final raw program count</th>
      <th>Starting program count</th>
      <th>Programs created</th>
      <th>Programs skipped</th>
      <th>Final program count</th>
      <th>Conversion completed</th>
    </tr>
  </thead>

  <tbody>
    <% @conversion_summaries.each do |conversion_summary| %>
      <tr id="<%= conversion_summary.id %>">
        <td><%= conversion_summary.created_at %></td>
        <td><%= conversion_summary.raw_channel_count %></td>
        <td><%= conversion_summary.final_raw_channel_count %></td>
        <td><%= conversion_summary.starting_channel_count %></td>
        <td><%= conversion_summary.channels_created %></td>
        <td><%= conversion_summary.channels_skipped %></td>
        <td><%= conversion_summary.final_channel_count %></td>
        <td><%= conversion_summary.raw_program_count %></td>
        <td><%= conversion_summary.final_raw_program_count %></td>
        <td><%= conversion_summary.starting_program_count %></td>
        <td><%= conversion_summary.programs_created %></td>
        <td><%= conversion_summary.programs_skipped %></td>
        <td><%= conversion_summary.final_program_count %></td>
        <td><%= conversion_summary.conversion_completed %></td> 
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @conversion_summaries, renderer: BootstrapPagination::Rails %>
