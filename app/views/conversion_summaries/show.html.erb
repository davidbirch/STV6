<p id="notice"><%= notice %></p>
<h3>Showing Conversion Summary</h3>
<div class="form-group">
   <div class="col-sm-2"><strong>ID:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.id %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Raw channel count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.raw_channel_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Final raw channel count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.final_raw_channel_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Starting channel count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.starting_channel_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Channels created:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.channels_created %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Channels skipped:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.channels_skipped %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Final channel count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.final_channel_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Raw program count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.raw_program_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Final raw program count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.final_raw_program_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Starting program count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.starting_program_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Programs created:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.programs_created %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Programs skipped:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.programs_skipped %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Final program count:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.final_program_count %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2"><strong>Conversion completed:</strong></div>
   <div class="col-sm-10"><%= @conversion_summary.conversion_completed %>&nbsp;</div>
</div>
<div class="form-group">
   <div class="col-sm-2">
      <%= link_to 'Back', conversion_summaries_path , :class => 'btn btn-xs btn-default'%>
   </div>
   <div class="col-sm-10"></div>
</div>
