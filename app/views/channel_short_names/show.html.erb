<%= render "shared/navbar" %>
<%= render "shared/breadcrumb",
locals: {
   active_level: 2,
   level_0_name: "Home Icon",
   level_0_link: root_path,
   level_1_name: "Channels By Short Name",
   level_1_link: channel_short_names_path,
   level_2_name: params[:url_friendly_short_name],
   level_2_link: nil
   }
%>
<div class="container">
  <div class="row">
    <div class="col-lg-9">          
      <%= render "shared/flash" %>
      <%= render "shared/content_header", locals: {header: "Short Name: " + params[:url_friendly_short_name]}  %>
      <%= render "channels/table_index" %>
    </div><!-- /col-lg-9 -->
    <div class="col-lg-3">
      <%= render "shared/sidebar" %>
    </div><!-- /col-lg-3 -->
  </div><!-- /row -->
</div> <!-- /container -->
<%= render "shared/footer" %>