<p id="notice"><%= notice %></p>
<h3>Showing Raw Channel</h3>
<div class="form-group">
   <div class="col-sm-2"><strong>ID:</strong></div>
   <div class="col-sm-10"><%= @raw_channel.id %></div>
</div>

<div class="form-group">
   <div class="col-sm-2"><strong>XMLTV ID:</strong></div>
   <div class="col-sm-10"><%= @raw_channel.xmltv_id %></div>
</div>

<div class="form-group">
   <div class="col-sm-2"><strong>Channel Name:</strong></div>
   <div class="col-sm-10"><%= @raw_channel.channel_name %></div>
</div>
<div class="form-group">
   <div class="col-sm-2">
      <%= link_to 'Edit', edit_raw_channel_path(@raw_channel), :class => 'btn btn-xs btn-default' %>
      <%= link_to 'Back', raw_channels_path , :class => 'btn btn-xs btn-default'%>
   </div>
   <div class="col-sm-10"></div>
</div>