<% unless defined?(locals[:header]).nil? %>
<h4><%= locals[:header] %></h4>
<% end %>    
<table class="table table-condensed">
  <thead>
    <tr>
      <th>#</th>
      <th>Channel Name</th>
      <th>Free Or Pay</th>
    </tr>
  </thead>

  <tbody>
    <% @raw_channels.each do |raw_channel| %>
      <tr id="raw_channel_<%= raw_channel.id %>">
        <td><%= link_to raw_channel.id, raw_channel%></td>
        <td><%= raw_channel.channel_name %></td>
        <td><%= raw_channel.free_or_pay %></td>    
      </tr>
    <% end %>
  </tbody>
</table>
