<%= semantic_form_for @channel do |f| %>
  <%= f.inputs do %>
    <%= f.input :xmltv_id %>
    <%= f.input :free_or_pay %>
    <%= f.input :name %>
    <%= f.input :short_name %>
    <%= f.input :black_flag %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
