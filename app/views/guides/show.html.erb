<% active_level = 0 %>
<% level_0_name = "Home Icon" %>
<% level_0_link = root_url %>
<% level_1_name = @region.name if @region %>
<% level_1_link = (root_url+@region.friendly_id) if @region %>
<% level_2_name = @sport.name if @sport %>
<% level_2_link = (root_url+@region.friendly_id+"/"+@sport.friendly_id) if @sport %>
<% header = nil %>
<%= render "shared/adsense_responsive_banner_1" %> 
<div class="container">
  <%= render "shared/adsense_responsive_banner_2" %>    
  <div class="row">
    <div class="col-xs-12 col-md-1 col-lg-2"></div>
    <div class="col-xs-12 col-md-10 col-lg-8">          
      <%= render "shared/flash" %>
      <%= render "form_control" %>
      <div class="visible-xs-block">
         <%= render "form_show_visible_xs" %>  
      </div>
       <div class="hidden-xs">
         <%= render "form_show_hidden_xs" %>  
      </div> 
    </div><!-- /col-xs-12 col-md-10 col-lg-8 -->
    <div class="col-xs-12 col-md-1 col-lg-2"></div>
  </div><!-- /row -->
</div> <!-- /container -->
<%= render "shared/adsense_responsive_banner_1" %> 
<%= render "schema_org_markup", locals: {active_level: active_level, level_0_name: level_0_name, level_0_link: level_0_link, level_1_name: level_1_name, level_1_link: level_1_link, level_2_name: level_2_name, level_2_link: level_2_link} %> 
